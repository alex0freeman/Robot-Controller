RC_TOOL(TOOL);
RC_REF_SYS(I001);
MOVJ P001, V50, Z0 ; # GO Back Home
SETOUT OUT1, 0;    # Close the Item
# Step 1
MOVL BP004, V50, Z0;
# Step 2: Communication with sensor
RC_COMMUNICATION(5000);
# Print the data from extern
RC_PRINT(KX, KY, KZ, KA, KB, KC);
# Add offset on X-axes
ADD KZ, 200.0;
SETE BP001(1), KX;
SETE BP001(2), KY;
SETE BP001(3), KZ;
SETE BP001(4), KC;
SETE BP001(5), KB;
SETE BP001(6), KA;
RC_PRINT(BP001);
# Step 3: open the Item
MOVJ BP001, V50, Z0;
SETOUT OUT1, 1;
# Step 4: grab the object
SUB BP001(3), 200.0;
MOVL BP001, V50, Z0;
SETOUT OUT1, 0;
# Step 5
ADD BP001(3), 200.0;
MOVL BP001, V50, Z0;
# Step 6: move the object
MOVJ BP002, V50, Z0;
# Step 7: put the object
MOVL BP003, V100, Z0;
SETOUT OUT1, 1;
# Step 8: raise
MOVL BP002, V50, Z0;
# Step 9: Go back home
MOVL P001, V50, Z0;

HALT;
